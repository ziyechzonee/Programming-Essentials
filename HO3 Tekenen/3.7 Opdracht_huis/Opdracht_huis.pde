void setup() {
  size(400, 400);
  background(200, 240, 255);
  noStroke();

  fill(255, 200, 150);
  rect(100, 200, 200, 150);

  fill(180, 100, 50);
  arc(200, 200, 220, 120, PI, 2*PI);

  fill(120, 70, 30);
  rect(180, 270, 40, 80);
  fill(255, 255, 0);
  ellipse(215, 310, 8, 8);

  fill(150, 200, 255);
  ellipse(150, 250, 40, 40);
  stroke(255);
  line(150, 230, 150, 270);
  line(130, 250, 170, 250);
}
